# NEW FEATURES                           #
##########################################

print('With var:')

var i = 10
print('Before: ' + str(i))

for (var i in range(1, 6, 1)) {
    print(str(i))
}

print('After: ' + str(i))



print('Without var:')

var i = 10
print('Before: ' + str(i))

for (i in range(1, 6, 1)) {
    print(str(i))
}

print('After: ' + str(i))



print(str(
    range(1, 11, 1)
))
print(str(
    range(-10, 0, 2)
))



