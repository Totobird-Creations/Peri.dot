# Added os default module
var sys = include('sys')

print(str(
    sys.terminalsize()
))

print(str(
    sys.args
))


# Added datetime default module
var datetime = include('datetime')

var d = datetime.datetime(y=2020, m=8, d=26, H=13, M=04, S=26, f=408279)

print(str(
    d
))

print(
    d.strftime('%a %b %d %H:%M:%S.%f %Y')
)

print(str(
    datetime.now()
))