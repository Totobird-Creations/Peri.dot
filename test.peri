# Added default arguments to functions

var greet = func(name: str, emph='!') -> str {
    var message = 'Hello, ' + name + emph
    print(message)
    return(message)
}
greet('Totobird', emph='!!!!!!!!!!')


var f = func(name=Null) -> Null {
    print(str(type(name)))
    return(Null)
}
f(name=10)


